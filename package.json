{
  "name": "home-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/home-api/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "8.1.1",
    "@nestjs/core": "8.1.1",
    "@nestjs/graphql": "9.0.6",
    "@nestjs/platform-fastify": "8.1.1",
    "@nestjs/typeorm": "8.0.2",
    "@types/axios": "0.14.0",
    "apollo-server-fastify": "3.4.0",
    "axios": "0.23.0",
    "class-transformer": "0.4.0",
    "graphql-subscriptions": "1.2.1",
    "lgtv2": "1.6.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "sqlite3": "5.0.2",
    "ssh2-promise": "1.0.2",
    "tiny-json-http": "7.3.1",
    "typeorm": "0.2.38"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.4",
    "@nestjs/testing": "8.1.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/lgtv2": "1.4.2",
    "@types/node": "14.17.27",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "jest": "27.3.0",
    "prettier": "2.4.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@homeapi/freebox/(.*)": "<rootDir>/libs/freebox/src/$1",
      "@homeapi/freebox": "<rootDir>/libs/freebox/src",
      "@homeapi/nordvpn/(.*)": "<rootDir>/libs/nordvpn/src/$1",
      "@homeapi/nordvpn": "<rootDir>/libs/nordvpn/src",
      "@homeapi/pubsub/(.*)": "<rootDir>/libs/pubsub/src/$1",
      "@homeapi/pubsub": "<rootDir>/libs/pubsub/src",
      "@homeapi/ovpn/(.*)": "<rootDir>/libs/ovpn/src/$1",
      "@homeapi/ovpn": "<rootDir>/libs/ovpn/src",
      "@homeapi/lgtv/(.*)": "<rootDir>/libs/lgtv/src/$1",
      "@homeapi/lgtv": "<rootDir>/libs/lgtv/src"
    }
  }
}
