{
  "name": "home-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/home-api/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.3.7",
    "@nestjs/platform-fastify": "^7.0.9",
    "@nestjs/typeorm": "^7.0.0",
    "@types/axios": "^0.14.0",
    "apollo-server-fastify": "^2.14.2",
    "axios": "^0.20.0",
    "class-transformer": "^0.3.1",
    "graphql-subscriptions": "^1.1.0",
    "lgtv2": "^1.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sqlite3": "^5.0.0",
    "ssh2-promise": "^0.1.7",
    "tiny-json-http": "^7.1.2",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.14",
    "@types/lgtv2": "^1.4.0",
    "@types/node": "^14.0.5",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@homeapi/freebox/(.*)": "<rootDir>/libs/freebox/src/$1",
      "@homeapi/freebox": "<rootDir>/libs/freebox/src",
      "@homeapi/nordvpn/(.*)": "<rootDir>/libs/nordvpn/src/$1",
      "@homeapi/nordvpn": "<rootDir>/libs/nordvpn/src",
      "@homeapi/pubsub/(.*)": "<rootDir>/libs/pubsub/src/$1",
      "@homeapi/pubsub": "<rootDir>/libs/pubsub/src",
      "@homeapi/ovpn/(.*)": "<rootDir>/libs/ovpn/src/$1",
      "@homeapi/ovpn": "<rootDir>/libs/ovpn/src",
      "@homeapi/lgtv/(.*)": "<rootDir>/libs/lgtv/src/$1",
      "@homeapi/lgtv": "<rootDir>/libs/lgtv/src"
    }
  }
}
