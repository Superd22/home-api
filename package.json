{
  "name": "home-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/home-api/test/jest-e2e.json",
    "infra:build": "npx nest build infra",
    "infra:synthonly": "node dist/apps/infra/main.js",
    "infra:synth": "yarn run infra:build && yarn run infra:synthonly",
    "infra:watch": "npx nest start --watch infra"
  },
  "dependencies": {
    "@actions/core": "1.8.2",
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@nestjs/graphql": "10.0.12",
    "@nestjs/platform-fastify": "8.4.5",
    "@nestjs/typeorm": "8.0.4",
    "@types/axios": "0.14.0",
    "@types/lodash": "4.14.182",
    "apollo-server-fastify": "3.7.0",
    "axios": "0.27.2",
    "cdk8s": "2.2.87",
    "cdk8s-plus-17": "1.0.0-beta.11",
    "class-transformer": "0.5.1",
    "constructs": "10.1.12",
    "exec-sh": "0.4.0",
    "graphql": "15.5.1",
    "graphql-subscriptions": "2.0.0",
    "lgtv2": "1.6.3",
    "lodash": "4.17.21",
    "nest-commander": "2.5.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "sqlite3": "5.0.8",
    "ssh2-promise": "1.0.2",
    "tiny-json-http": "7.4.2",
    "typeorm": "0.3.6",
    "yaml": "2.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.6",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.5",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.1",
    "@types/lgtv2": "1.4.2",
    "@types/node": "16.11.36",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "ts-jest": "28.0.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@homeapi/freebox/(.*)": "<rootDir>/libs/freebox/src/$1",
      "@homeapi/freebox": "<rootDir>/libs/freebox/src",
      "@homeapi/nordvpn/(.*)": "<rootDir>/libs/nordvpn/src/$1",
      "@homeapi/nordvpn": "<rootDir>/libs/nordvpn/src",
      "@homeapi/pubsub/(.*)": "<rootDir>/libs/pubsub/src/$1",
      "@homeapi/pubsub": "<rootDir>/libs/pubsub/src",
      "@homeapi/ovpn/(.*)": "<rootDir>/libs/ovpn/src/$1",
      "@homeapi/ovpn": "<rootDir>/libs/ovpn/src",
      "@homeapi/lgtv/(.*)": "<rootDir>/libs/lgtv/src/$1",
      "@homeapi/lgtv": "<rootDir>/libs/lgtv/src",
      "@homeapi/k8s/(.*)": "<rootDir>/libs/k8s/src/$1",
      "@homeapi/k8s": "<rootDir>/libs/k8s/src",
      "^@homeapi/ctsdk(|/.*)$": "<rootDir>/libs/ctsdk/src/$1"
    }
  }
}
