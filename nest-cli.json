{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/home-api/src",
  "monorepo": true,
  "root": "apps/home-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/home-api/tsconfig.app.json"
  },
  "projects": {
    "home-api": {
      "type": "application",
      "root": "apps/home-api",
