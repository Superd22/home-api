      "root": "apps/infra",
      "entryFile": "main",
      "sourceRoot": "apps/infra/src",
      "compilerOptions": {
        "tsConfigPath": "apps/infra/tsconfig.app.json"
      }
    },
    "freebox": {
      "type": "library",
      "root": "libs/freebox",
      "entryFile": "index",
